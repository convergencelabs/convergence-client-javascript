{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "outDir": "build",
    "sourceMap": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "importHelpers": true
  },
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "bower_components"
  ]
}